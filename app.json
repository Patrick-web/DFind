{
  "expo": {
    "name": "DFound",
    "slug": "DFound",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/appImages/icon.png",
    "scheme": "dfound",
    "userInterfaceStyle": "automatic",
    "jsEngine": "hermes",
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app requires access to your photos"
        }
      ]
    ],
    "splash": {
      "image": "./assets/images/appImages/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFEDCF"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/appImages/adaptive-icon.png",
        "backgroundColor": "#FFEDCF"
      },
      "package": "com.pntx.dfound"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    }
  }
}
